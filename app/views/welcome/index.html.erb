<div id="carousel-example-generic" class="carousel slide headerslider" data-ride="carousel">
  <div class="carousel-inner" role="listbox">
    <div class="item active"> <%= link_to image_tag("sliderimg1.png", :alt => "")%>
      <div class="carousel-caption"> </div>
    </div>
    <div class="item"> <%= link_to image_tag("sliderimg2.png", :alt => "")%>
      <div class="carousel-caption"> </div>
    </div>
    <div class="item"> <%= link_to image_tag("sliderimg3.png", :alt => "")%>
      <div class="carousel-caption"> </div>
    </div>
  </div>
</div>
<section id="bannersection" class="row">
  <div class="col-md-12">
    <div class="container">
      <div class="heading">
        <h1>
          <center>
            Find your Moment
          </center>
        </h1>
        <h3>
          <center>
            Bars, Clubs and Lounges
          </center>
        </h3>
        <h3> </h3>
      </div>
      <div class="searchbar">
        <div class="row center-block">
          <div class="bs-example" data-example-id="text-form-control">
            <div class="input-group inner-addon left-addon">
              <input type="text" class="form-control search_icon" placeholder="Name of city...">
              <span class="input-group-btn">
              <button class="btn btn-blue" type="button">Venues</button>
              <button class="btn btn-blue active" type="button">Parties</button>
              </span>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
<section id="venuesection" class="row venuesection">
  <div class="container">
    <div class="row">
      <div class="title">
        <h1>
          <center>
            Venues
          </center>
        </h1>
        <h3>
          <center class="bottomtagline">
            Begin your night in the right place.
          </center>
        </h3>
      </div>
      <div class="sub">
        <div class="row center-block">
          <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 center-block socialize">
            <div class="color_block">
              <div class="blue_block">
                <span class="mid_text">Socialize</span>
              </div>
            </div>
          </div>
          <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 center-block venuecategory">
            <center>
              <a href="#">
              <h2>Clubs</h2>
              </a>
            </center>
          </div>
          <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 center-block engage">
            <div class="color_block">
              <div class="green_block">
                <span class="mid_text">Engage</span>
              </div>
            </div>
          </div>
        </div>
        <div class="row center-block">
          <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 center-block venuecategory">
            <center>
              <a href="#">
              <h2>Bars</h2>
              </a>
            </center>
          </div>
          <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 center-block dance">
            <div class="color_block">
              <div class="purple_block">
                <span class="mid_text">Dance</span>
              </div>
            </div>
          </div>
          <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 center-block venuecategory">
            <center>
              <a href="#">
              <h2>Lounges</h2>
              </a>
            </center>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
<section id="partysection" class="row partiesection">
  <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
    <div class="container">
      <div class="title">
        <h1>
          <center>
            Parties
          </center>
        </h1>
        <h3>
          <center class="bottomtagline">
            Plan ahead.
          </center>
        </h3>
        <h3> </h3>
      </div>
      <div class="sub">
        <div class="row center-block">
          <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
            <center>
              <%= link_to image_tag("1_love.png", :alt => "")%>
              <h2>Special</h2>
            </center>
          </div>
          <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
            <center>
              <%= link_to image_tag("2_masks.png", :alt => "")%>
              <h2>Theme</h2>
            </center>
          </div>
          <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
            <center>
              <%= link_to image_tag("3_lights.png", :alt => "")%>
              <h2>Holiday</h2>
            </center>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
